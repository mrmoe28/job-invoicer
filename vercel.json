{
  "framework": "nextjs",
  "buildCommand": "cd apps/web && pnpm install && pnpm run build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "pnpm install",
  "regions": ["iad1"],
  "functions": {
    "apps/web/app/api/**/*.ts": {
      "runtime": "nodejs20.x",
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
